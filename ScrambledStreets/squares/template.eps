/black {0 setgray fill} def

/white {gsave 1 setgray fill grestore lnw setlinewidth stroke} def

/gray {gsave 0.65 setgray fill grestore lnw setlinewidth stroke} def

/colors {
	/param exch def
	param 0 eq {black} if
	param 1 eq {gray} if
	param 2 eq {white} if
	%stroke not necessary
} def

/horizflip {width 0 translate -1 1 scale} def

/vertflip {0 height translate 1 -1 scale} def

/orient {
	/n exch def
	n 1 eq n 2 eq or {vertflip} if
	n 2 eq n 3 eq or {horizflip} if
} def

/height 1050 def
/width height def
/third height 3 div def
/halfline 20 def
/xcontrolpoint 300 def
/ycontrolpoint 300 def
/lnw 4 def

38 38 translate

/smallcurve {
/color exch def
/orientation exch def
gsave
orientation orient
%%small curve to the left
newpath
third 0 moveto
third halfline sub    0 lineto
third halfline sub    ycontrolpoint halfline sub
	xcontrolpoint halfline sub    third halfline sub
		0    third halfline sub
		curveto
0    third halfline add lineto
xcontrolpoint halfline add   third halfline add
	third halfline add    ycontrolpoint halfline add
		third halfline add    0
		curveto
closepath
color colors
grestore
} def


/tallcurve {
/color exch def
/orientation exch def
gsave
orientation orient
%tall curve to the left
newpath
third    0 moveto
third halfline sub    0 lineto
third halfline sub    ycontrolpoint 2 mul halfline sub
	xcontrolpoint halfline sub    height third sub halfline sub
		0    height third sub halfline sub
		curveto
0    height third sub halfline add lineto
xcontrolpoint halfline add    height third sub halfline add
	third halfline add    ycontrolpoint 2 mul halfline add
		third halfline add    0
		curveto
closepath
color colors
grestore
} def


/diag {
/color exch def
/orientation exch def
gsave
orientation orient
%diagonal from UR to BL
newpath
width    height third sub moveto
width    height third sub halfline add lineto
width xcontrolpoint 0.9 mul sub    height third sub halfline add
	xcontrolpoint 0.9 mul    third halfline add
		0    third halfline add
		curveto
0    third halfline sub lineto
xcontrolpoint 0.9 mul    third halfline sub
	width xcontrolpoint 0.9 mul sub    height third sub halfline sub
		width    height third sub halfline sub
		curveto

closepath
color colors
grestore
} def


/semicirc {
/color exch def
/orientation exch def
gsave
orientation orient
%left-side curve
newpath
0    height third sub moveto
0    height third sub halfline add lineto
xcontrolpoint 1.2 mul halfline add   height third sub halfline add
	xcontrolpoint 1.2 mul  halfline add    third halfline sub
		0    third halfline sub
		curveto
0    third halfline add lineto
xcontrolpoint  1.2 mul halfline sub    third halfline add
	xcontrolpoint 1.2 mul  halfline sub    height third sub halfline sub
		0    height third sub halfline sub
		curveto

closepath
color colors
grestore
} def


/horizline {
/color exch def
/orientation exch def
gsave
orientation orient
%low horizontal line
newpath
width    third halfline add moveto
0    third halfline add lineto
0    third halfline sub lineto
width third halfline sub lineto
closepath
color colors
grestore
} def

/vertline {
/color exch def
/orientation exch def
gsave
orientation orient
%straight down the middle
newpath
third    height moveto
third halfline add    height lineto
third halfline add    0 lineto
third halfline sub    0 lineto
third halfline sub    height lineto
closepath
color colors
grestore
} def

gsave
0 orient
%straight down the middle
newpath
third    height moveto
third halfline add    height lineto
third halfline add    0 lineto
third halfline sub    0 lineto
third halfline sub    height lineto
closepath
0 colors
grestore

2 0 vertline
0 0 smallcurve 
3 1 tallcurve
1 2 diag
0 1 horizline
1 0 semicirc
